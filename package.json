{
  "name": "webrtc_mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "pod": "cd ios && pod install && cd ..",
    "postinstall": "./node_modules/.bin/husky install",
    "cleanup": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && npm cache clean --force",
    "mount": "sh bin/mount.sh",
    "unmount": "sh bin/unmount.sh",
    "cli": "node node_modules/react-native/local-cli/cli.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "cold-start": "yarn start --reset-cache",
    "gradlew-clean": "cd android && ./gradlew clean",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js --ext .js,.jsx,.ts,.tsx .",
    "format": "./node_modules/.bin/eslint --fix -c .eslintrc.js --ext .js,.jsx,.ts,.tsx .",
    "prettier": "./node_modules/.bin/prettier --write \"{src,__{tests,mocks}__}/**/*{.js,.jsx,.ts,.tsx}\"",
    "android-shake": "adb shell input keyevent 82",
    "android": "yarn cli run-android",
    "android-release": "yarn cli run-android --variant=release",
    "android-build-release-aab": "cd android && chmod +x gradlew && ./gradlew bundleRelease",
    "android-build-release": "cd android && chmod +x gradlew && ./gradlew app:assembleRelease --no-daemon",
    "android-build-debug": "cd android && chmod +x gradlew && ./gradlew app:assembleDebug --no-daemon",
    "ios": "yarn cli run-ios",
    "ios-release": "yarn cli run-ios --configuration Release",
    "ios-build-release": "cd ios && xcode-select -p && pod install && xcodebuild -workspace webrtc_mobile.xcworkspace -scheme webrtc_mobile clean archive -sdk iphoneos -configuration Release -UseModernBuildSystem=NO -archivePath $PWD/webrtc_mobile CODE_SIGNING_ALLOWED=NO",
    "ios-build-debug": "cd ios && xcode-select -p && pod install && xcodebuild -workspace webrtc_mobile.xcworkspace -scheme webrtc_mobile clean archive -sdk iphoneos -configuration Debug -UseModernBuildSystem=NO -archivePath $PWD/webrtc_mobile CODE_SIGNING_ALLOWED=NO",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-firebase/analytics": "^17.3.1",
    "@react-native-firebase/app": "^17.3.1",
    "@react-native-firebase/crashlytics": "^17.3.1",
    "@react-native-firebase/messaging": "^17.3.1",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "color": "^4.2.3",
    "react": "18.2.0",
    "react-native": "0.71.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-mmkv": "^2.6.2",
    "react-native-permissions": "^3.7.2",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/color": "^3.0.3",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "5.1.6",
    "@types/styled-components-react-native": "^5.2.1",
    "babel-jest": "^29.2.1",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.8.4",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
